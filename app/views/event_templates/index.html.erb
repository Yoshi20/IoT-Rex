<p id="notice"><%= notice %></p>

<h1>Event Templates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Static data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_templates.each do |event_template| %>
      <tr>
        <td><%= event_template.name %></td>
        <td><%= event_template.static_data %></td>
        <td><%= link_to 'Show', event_template %></td>
        <td><%= link_to 'Edit', edit_event_template_path(event_template) %></td>
        <td><%= link_to 'Destroy', event_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Template', new_event_template_path %>
